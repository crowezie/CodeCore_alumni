<%= simple_form_for @profile do |p| %>
  <%= p.simple_fields_for :asset_attributes do |f| %>
  <%= f.input        :linkedin, label: "Linkedin" %>
  <%= f.input        :github, label: "GitHub" %>
  <%= f.input        :twitter, label: "Twitter" %>
  <%= f.input        :resume, label: "Resume" %>
  <%#= f.hidden_field :profile_id, value: current_user.profile.id %>
  <% end %>
  <%= p.submit "Submit", class: "btn btn-info" %>
<%end%>
